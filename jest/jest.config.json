{
  "collectCoverageFrom": ["../projects/lib/src/**/*.{ts,js}"],
  "moduleFileExtensions": ["ts", "js", "json"],
  "moduleNameMapper": {},
  "roots": ["<rootDir>/../projects/lib/src"],
  "testEnvironment": "node",
  "testRegex": "test\\.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "transformIgnorePatterns": ["node_modules*"],
  "setupFilesAfterEnv": ["<rootDir>/setup.ts"]
}
